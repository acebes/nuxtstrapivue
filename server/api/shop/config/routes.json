{
  "routes": [
    {
      "method": "GET",
      "path": "/shops",
      "handler": "Shop.find",
      "config": {
        "policies": []
      }
    },
    {
      "method": "GET",
      "path": "/shops/count",
      "handler": "Shop.count",
      "config": {
        "policies": []
      }
    },
    {
      "method": "GET",
      "path": "/shops/:id",
      "handler": "Shop.findOne",
      "config": {
        "policies": []
      }
    },
    {
      "method": "POST",
      "path": "/shops",
      "handler": "Shop.create",
      "config": {
        "policies": []
      }
    },
    {
      "method": "PUT",
      "path": "/shops/:id",
      "handler": "Shop.update",
      "config": {
        "policies": []
      }
    },
    {
      "method": "DELETE",
      "path": "/shops/:id",
      "handler": "Shop.destroy",
      "config": {
        "policies": []
      }
    },
    {
      "method": "POST",
      "path": "/shops/:id/relationships/:relation",
      "handler": "Shop.createRelation",
      "config": {
        "policies": []
      }
    },
    {
      "method": "PUT",
      "path": "/shops/:id/relationships/:relation",
      "handler": "Shop.updateRelation",
      "config": {
        "policies": []
      }
    },
    {
      "method": "DELETE",
      "path": "/shops/:id/relationships/:relation",
      "handler": "Shop.destroyRelation",
      "config": {
        "policies": []
      }
    }
  ]
